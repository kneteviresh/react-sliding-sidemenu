{
  "name": "react-sliding-sidemenu",
  "version": "1.0.2",
  "description": "Customisable Responsive sliding sidebar menu , can be easily intigrated within application.",
  "main": "lib/index.js",
  "scripts": {
    "build:npm": "babel --presets=react,es2015,stage-2 src --out-dir lib && node-sass  src/styles/ -o lib/styles/",
    "start": "npm run build",
    "build": "webpack -d && webpack-dev-server --content-base dist/app/ --inline --hot --port 3005 --history-api-fallback",
    "build:prod": "webpack -p"
  },
  "keywords": [
    "sidebar",
    "sidemenu",
    "react sidemenu",
    "react sidebar",
    "sliding sidemenu",
    "sliding sidebar",
    "responsive sidebar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kneteviresh/react-sliding-sidemenu"
  },
  "homepage": "https://github.com/kneteviresh/react-sliding-sidemenu#readme",
  "directories": {
    "src": "Source code"
  },
  "author": "Veeresh Nete",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  }
}
